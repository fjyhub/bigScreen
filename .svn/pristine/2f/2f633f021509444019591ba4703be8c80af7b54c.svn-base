.title{
  font-size: 30px;
  font-weight: normal;
  background-image: url('../../assets/imgs/1028.png');
  background-repeat: no-repeat;
  background-position-x: left;
  background-position-y: center;
  padding-left: 60px;
  span{    
    background: linear-gradient(to bottom, #7bcdd9, #0ea6d7);
    -webkit-background-clip: text;
    color: transparent;
  }
}
.title2{
  font-size: 30px;
  font-weight: normal;
  background-image: url('../../assets/imgs/1028.png');
  background-repeat: no-repeat;
  background-position-x: left;
  background-position-y: center;
  padding-left: 60px;
  span{    
    background: linear-gradient(to bottom, #a7f6ff, #5ad8ff);
    -webkit-background-clip: text;
    color: transparent;
  }
}
.title3{
  position: relative;
  color: #befffd;
  padding-left: 20px;
  font-size: 28px;
  font-weight: normal;
}
.title3::before{
  content: '';
  position: absolute;
  left: 0;
  bottom: 0;
  height: 28px;
  width: 8px;
  background: #befffd;
}

